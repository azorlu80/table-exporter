{
  "name": "table-exporter",
  "version": "1.0.0",
  "description": "Export web tables to CSV/Excel when built-in export fails",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --enable-logging",
    "build": "electron-builder",
    "build-win": "electron-builder --win --x64",
    "build-linux": "electron-builder --linux",
    "pack": "electron-builder --dir"
  },
  "keywords": [
    "table",
    "export",
    "csv",
    "excel",
    "extjs",
    "web-scraping",
    "data-extraction",
    "electron",
    "desktop-app"
  ],
  "author": "Ali",
  "license": "MIT",
  "devDependencies": {
    "electron": "^39.1.0"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "winston": "^3.18.3"
  }
}
